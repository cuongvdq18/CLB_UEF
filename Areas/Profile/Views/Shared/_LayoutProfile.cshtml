    <script src="~/Areas/Profile/Resource/js/vendor/modernizr-2.8.3.min.js"></script>
    @RenderSection("header", required: false)
</head>
<body>
    @if (Request.IsAuthenticated)
    {
        <div class="left-sidebar-pro">
            <nav id="sidebar" class="">
                <div class="sidebar-header">
                    <a href="@Url.Action("Index", "Dashboard")"><img class="main-logo" src="~/Areas/Profile/Resource/img/logo/logo.png" alt="" /></a>
                    <strong><a href="index.html">UEF</a></strong>
                </div>
                <div class="left-custom-menu-adp-wrap comment-scrollbar">
                    <nav class="sidebar-nav left-sidebar-menu-pro">

                        <ul class="metismenu" id="menu1">
                            <li>
                                <a href="@Url.Action("Index", "Dashboard")">
                                    <span class="educate-icon educate-home icon-wrap"></span>
                                    <span class="mini-click-non">Trang chủ</span>
                                </a>

                            </li>
                            <li>
                                <a href="@Url.Action("DangKyCLB", "ClubUser", new { id = @Session["UserId"] })" aria-expanded="false"><span class="educate-icon educate-library icon-wrap"></span> <span class="mini-click-non">Tham gia CLB</span></a>
                            </li>

                            <li>
                                <a href="@Url.Action("DangKyTLCLB", "ClubUser", new { id = @Session["UserId"] })" aria-expanded="false"><span class="educate-icon educate-apps icon-wrap"></span> <span class="mini-click-non">Đăng ký CLB</span></a>
                            </li>
                            @using (var context = new ApplicationDbContext())
                            {
                                var dbrole = Session["Role"] as List<DBRoles>;
                                if (dbrole != null)
                                {
                                    foreach (var role in dbrole)
                                    {
                                        if (role.Name == "Member")
                                        {
                                            <li>
                                                <a href="@Url.Action("DSCLB", "ClubUser", new { id = @Session["UserId"] })">
                                                    <span class="educate-icon educate-nav icon-wrap"></span>
                                                    <span class="mini-click-non">Câu lạc bộ</span>
                                                </a>

                                            </li>
                                            <li>
                                                <a class="has-arrow" aria-expanded="false"><span class="educate-icon educate-event icon-wrap sub-icon-mg"></span> <span class="mini-click-non">Hoạt động</span></a>
                                                <ul class="submenu-angle" aria-expanded="false">
                                                    <li><a title="Sự kiện" href="@Url.Action("SuKien_CLB", "ClubUser", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Sự kiện</span></a></li>
                                                    <li><a title="Hoạt động" href="@Url.Action("HoatDong_CLB", "ClubUser", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Hoạt động</span></a></li>
                                                    <li><a title="Lịch hoạt động" href="@Url.Action("LichTap_CLB", "ClubUser", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Lịch hoạt động</span></a></li>
                                                    <li><a title="Thông báo" href="@Url.Action("ThongBao_CLB", "ClubUser", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Thông báo</span></a></li>
                                                    <li><a title="Nhật ký" href="@Url.Action("NhatKyCLB", "ClubUser", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Nhật ký</span></a></li>

                                                </ul>
                                            </li>
                                            <li>
                                                <a href="@Url.Action("listCLB", "NhiemVU", new { id = @Session["UserId"] })" aria-expanded="false"><span class="educate-icon educate-student icon-wrap"></span> <span class="mini-click-non">Nhiệm vụ</span></a>
                                            </li>
                                            <li>
                                                <a href="@Url.Action("PhanHoiCLB", "ClubUser", new { id = @Session["UserId"] })" aria-expanded="false"><span class="educate-icon educate-form icon-wrap"></span> <span class="mini-click-non">Phản hồi</span></a>
                                            </li>
                                        }
                                        if (role.Name == "HLV")
                                        {
                                            <li>
                                                <a class="has-arrow" href="all-courses.html" aria-expanded="false"><span class="educate-icon educate-course icon-wrap"></span> <span class="mini-click-non">Quản lý CLB</span></a>
                                                <ul class="submenu-angle" aria-expanded="false">
                                                    <li><a title="Câu lạc bộ" href="@Url.Action("QuanLyCLB_DSCLB", "QuanLyCLB", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Câu lạc bộ</span></a></li>
                                                    <li><a title="Thành viên" href="@Url.Action("CLBQL", "QuanLyThanhVien", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Thành viên</span></a></li>
                                                    <li><a title="Phản hồi" href="@Url.Action("QLPhanHoi_CLB", "QuanLyPhanHoi", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Phản hồi</span></a></li>
                                                    <li><a title="Xét duyệt thành viên" href="@Url.Action("QLXetDuyetTV_CLB", "QuanLyXetDuyetDKTV", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Xét duyệt thành viên</span></a></li>

                                                </ul>
                                            </li>

                                            <li>
                                                <a class="has-arrow" href="mailbox.html" aria-expanded="false"><span class="educate-icon educate-data-table icon-wrap"></span> <span class="mini-click-non">Quản lý hoạt động</span></a>
                                                <ul class="submenu-angle" aria-expanded="false">
                                                    <li><a title="Hoạt động" href="@Url.Action("QLHoatDong_CLB", "QuanLyHoatDong", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Hoạt động</span></a></li>
                                                    <li><a title="Sự kiện" href="@Url.Action("QLSuKien_CLB", "QuanLySuKien", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Sự kiện</span></a></li>
                                                    <li><a title="Thông báo" href="@Url.Action("QLTBCLB", "QuanLyThongBao", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Thông báo</span></a></li>
                                                    <li><a title="Lịch hoạt động" href="@Url.Action("QLLichTap_CLB", "QuanLyLichTap", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Lịch hoạt động</span></a></li>
                                                    <li><a title="Nhiệm vụ" href="@Url.Action("QLNhiemVu_CLB", "QuanLyNhiemVu", new { id = @Session["UserId"] })"><span class="mini-sub-pro">Nhiệm vụ</span></a></li>
                                                </ul>
                                            </li>

                                        }
                                        if (role.Name == "Admin")
                                        {
                                            <li>
                                                <a href="@Url.Action("QLDkyCLB", "QuanLyDkyCLB", new { id = @Session["UserId"] })" aria-expanded="false"><span class="educate-icon educate-apps icon-wrap"></span> <span class="mini-click-non">Xét duyệt CLB</span></a>
                                            </li>

                                        }
                                    }
                                }
                            }
                        </ul>
                    </nav>
                </div>
            </nav>
        </div>

    }
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="img/logo/logo.png" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-advance-area">
            <div class="header-top-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="header-top-wraper">
                                <div class="row">
                                    <div class="col-lg-1 col-md-0 col-sm-1 col-xs-12">
                                        <div class="menu-switcher-pro">
                                            <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                                                <i class="educate-icon educate-nav"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-7 col-sm-6 col-xs-12">

                                    </div>
                                    <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                        <div class="header-right-info">
                                            <ul class="nav navbar-nav mai-top-nav header-right-menu">
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                                        <img style="height:26px;width:30px;" src="@Session["AnhDaiDien"]" alt="" />
                                                        <span class="admin-name">@Session["Ho"] @Session["Ten"]</span>
                                                        <i class="fa fa-angle-down edu-icon edu-down-arrow"></i>
                                                    </a>
                                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated zoomIn">
                                                        @using (var context = new ApplicationDbContext())
                                                        {
                                                            var dbrole = Session["Role"] as List<DBRoles>;
                                                            if (dbrole != null)
                                                            {
                                                                foreach (var role in dbrole)
                                                                {
                                                                    if (role.Name == "Admin")
                                                                    {
                                                                        <li>
                                                                            <a href="@Url.Action("Index", "TrangChu", new { area = "Admin" })"><span class="edu-icon edu-user-rounded author-log-ic"></span>Chuyển sang Admin</a>
                                                                        </li>
                                                                    }
                                                                }
                                                            }
                                                        }
                                                                        <li>
                                                                            <a href="@Url.Action("Index", "QLTaiKhoan", new { id = @Session["UserId"] })"><span class="edu-icon edu-home-admin author-log-ic"></span>Tài khoản</a>
                                                                        </li>
                                                                        <li>
                                                                            <a href="@Url.Action("HoSo", "ThongTinTV", new { id = @Session["UserId"] })"><span class="edu-icon edu-user-rounded author-log-ic"></span>Hồ sơ</a>
                                                                        </li>

                                                                        <li>
                                                                            <a href="@Url.Action("SignOut","Account",new { area = "" })"><span class="edu-icon edu-locked author-log-ic"></span>Đăng xuất</a>
                                                                        </li>
                                                                    </ul>
                                                </li>

                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
    <script src="~/Areas/Profile/Resource/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/bootstrap.js"></script>
    <script src="~/Areas/Profile/Resource/js/bootstrap.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/wow.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/jquery-price-slider.js"></script>
    <script src="~/Areas/Profile/Resource/js/jquery.meanmenu.js"></script>
    <script src="~/Areas/Profile/Resource/js/owl.carousel.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/jquery.sticky.js"></script>
    <script src="~/Areas/Profile/Resource/js/jquery.scrollUp.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/scrollbar/mCustomScrollbar-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/metisMenu/metisMenu.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/metisMenu/metisMenu-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/bootstrap-table.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/tableExport.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/data-table-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/bootstrap-table-editable.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/bootstrap-editable.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/bootstrap-table-resizable.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/colResizable-1.5.source.js"></script>
    <script src="~/Areas/Profile/Resource/js/data-table/bootstrap-table-export.js"></script>
    <script src="~/Areas/Profile/Resource/js/chart/jquery.peity.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/peity/peity-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/tab.js"></script>
    <script src="~/Areas/Profile/Resource/js/notifications/Lobibox.js"></script>
    <script src="~/Areas/Profile/Resource/js/notifications/notification-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/jquery.mockjax.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/mock-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/select2.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/moment.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/bootstrap-datetimepicker.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/bootstrap-editable.js"></script>
    <script src="~/Areas/Profile/Resource/js/editable/xediable-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/sparkline/jquery.charts-sparkline.js"></script>
    <script src="~/Areas/Profile/Resource/js/sparkline/sparkline-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/calendar/moment.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/calendar/fullcalendar.min.js"></script>
    <script src="~/Areas/Profile/Resource/js/calendar/fullcalendar-active.js"></script>
    <script src="~/Areas/Profile/Resource/js/plugins.js"></script>
    <script src="~/Areas/Profile/Resource/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
