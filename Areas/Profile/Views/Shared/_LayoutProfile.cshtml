<!DOCTYPE html>
<html>
@using ClubPortalMS.Models

<head>
    <title>@Page.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/owl.carousel.css">
