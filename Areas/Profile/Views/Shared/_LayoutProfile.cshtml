    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/calendar/fullcalendar.min.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/calendar/fullcalendar.print.min.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/editor/select2.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/editor/datetimepicker.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/editor/bootstrap-editable.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/editor/x-editor-style.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/notifications/Lobibox.min.css">
    <link rel="stylesheet" href="~/Areas/Profile/Resource/css/notifications/notifications.css">
